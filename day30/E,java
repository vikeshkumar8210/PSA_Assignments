package day30;

import java.io.File;
// If the folder is created it will return boolean value true or else it will return boolean value false. It will not replace existing folder.
// create new folder
public class E {
	public static void main(String[] args) {
		File f = new File ("C://files//p1");
		boolean val = f.mkdir();
		System.out.println(val);
	}

}

// OutPut:- True
